      <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js"></script>
    </div>
  </section>

  <!-- Section Partenaires -->
  <section class="partners-section py-5 bg-light shadow animate__animated animate__fadeIn">
    <div class="container">
      <h3 class="text-center text-danger fw-bolder mb-4 animate__animated animate__fadeInDown">Nos partenaires</h3>
      <div class="partners-carousel-wrapper position-relative">
        <button class="btn btn-light partners-scroll-btn start-0" id="partners-scroll-left" style="position: absolute; top: 50%; transform: translateY(-50%); z-index: 2;">
          <i class="fa fa-chevron-left"></i>
        </button>
        <div class="partners-carousel d-flex flex-row overflow-auto" id="partners-carousel" style="scroll-behavior: smooth; gap: 2rem; padding: 1rem 2.5rem;">
          <div class="flex-shrink-0 text-center" style="width: 140px;">
            <img src="https://upload.wikimedia.org/wikipedia/commons/4/44/Microsoft_logo.svg" alt="Microsoft" class="img-fluid" style="max-height: 60px; filter: grayscale(100%);">
          </div>
          <div class="flex-shrink-0 text-center" style="width: 140px;">
            <img src="https://upload.wikimedia.org/wikipedia/commons/2/2f/Google_2015_logo.svg" alt="Google" class="img-fluid" style="max-height: 60px; filter: grayscale(100%);">
          </div>
          <div class="flex-shrink-0 text-center" style="width: 140px;">
            <img src="https://upload.wikimedia.org/wikipedia/commons/a/ab/Logo_TV_2015.png" alt="TechVision" class="img-fluid" style="max-height: 60px; filter: grayscale(100%);">
          </div>
          <div class="flex-shrink-0 text-center" style="width: 140px;">
            <img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcSbVHFH9_bwUgXAoMrAPeYoPBU1wq2fWRdDQHFc5k9DIPg7hSdoq0zHeUA&usqp=CAE&s" alt="UNESCO" class="img-fluid" style="max-height: 60px; filter: grayscale(100%);">
          </div>
          <div class="flex-shrink-0 text-center" style="width: 140px;">
            <img src="https://upload.wikimedia.org/wikipedia/commons/5/51/IBM_logo.svg" alt="IBM" class="img-fluid" style="max-height: 60px; filter: grayscale(100%);">
          </div>
          <div class="flex-shrink-0 text-center" style="width: 140px;">
            <img src="data:image/png;base64," alt="AWS" class="img-fluid" style="max-height: 60px; filter: grayscale(100%);">
          </div>
          <div class="flex-shrink-0 text-center" style="width: 140px;">
            <img src="https://upload.wikimedia.org/wikipedia/commons/5/51/Facebook_f_logo_%282019%29.svg" alt="Facebook" class="img-fluid" style="max-height: 60px; filter: grayscale(100%);">
          </div>
          <div class="flex-shrink-0 text-center" style="width: 140px;">
            <img src="data:image/png;base64," alt="Tesla" class="img-fluid" style="max-height: 60px; filter: grayscale(100%);">
          </div>
          <div class="flex-shrink-0 text-center" style="width: 140px;">
            <img src="data:image/png;base64," alt="LinkedIn" class="img-fluid" style="max-height: 60px; filter: grayscale(100%);">
          </div>
        </div>
        <button class="btn btn-light shadow-sm partners-scroll-btn end-0" id="partners-scroll-right" style="position: absolute; top: 50%; right: 0; transform: translateY(-50%); z-index: 2;">
          <i class="fa fa-chevron-right"></i>
        </button>
      </div>
    </div>
    <style>
      .partners-carousel::-webkit-scrollbar { display: none; }
      .partners-carousel { -ms-overflow-style: none; scrollbar-width: none; }
      .partners-scroll-btn { background: rgba(255,255,255,0.8); border-radius: 50%; width: 40px; height: 40px; display: flex; align-items: center; justify-content: center; border: none; box-shadow: 0 2px 8px rgba(0,0,0,0.08);}
      .partners-scroll-btn:active { background: #f0f0f0; }
    </style>
    <script>
      document.addEventListener('DOMContentLoaded', function() {
        const carousel = document.getElementById('partners-carousel');
        document.getElementById('partners-scroll-left').onclick = function() {
          carousel.scrollBy({left: -200, behavior: 'smooth'});
        };
        document.getElementById('partners-scroll-right').onclick = function() {
          carousel.scrollBy({left: 200, behavior: 'smooth'});
        };
      });
    </script>
  </section>

  <!-- Section Pourquoi nous choisir -->
