  <!-- Section Cours -->
  <section id="courses" class="course-section py-5 bg-light animate__animated animate__fadeIn">
    <div class="container">
      <h3 class="text-center mb-4 animate__animated animate__fadeInDown">Cours populaires</h3>
      <div class="text-center mb-4"><img src="https://cdn-icons-png.flaticon.com/512/190/190411.png" alt="Cours Icon" style="width: 50px;"></div>
        <p class="text-center mb-4">Découvrez nos cours les plus populaires pour améliorer vos compétences et booster votre carrière.</p>
      <div class="row">
        <div class="col-md-3 mb-4">
          <div class="card h-100 shadow-sm animate__animated animate__zoomIn">
            <img src="https://images.unsplash.com/photo-1519125323398-675f0ddb6308?auto=format&fit=crop&w=400&q=80" class="card-img-top" alt="Cours 1">
            <div class="card-body">
              <h5 class="card-title">Introduction au développement mobile</h5>
              <p class="card-text">Lancez votre carrière dans le développement d'applications mobiles avec ce cours pour débutants.</p>
              <a href="#" class="btn btn-primary btn-sm">En savoir plus</a>
            </div>
          </div>
        </div>
        <div class="col-md-3 mb-4">
          <div class="card h-100 shadow-sm animate__animated animate__zoomIn animate__delay-1s">
            <img src="https://images.unsplash.com/photo-1461749280684-dccba630e2f6?auto=format&fit=crop&w=400&q=80" class="card-img-top" alt="Cours 2">
            <div class="card-body">
              <h5 class="card-title">Bases du design UI/UX</h5>
              <p class="card-text">Maîtrisez les fondamentaux de l'interface utilisateur et de l'expérience utilisateur.</p>
              <a href="#" class="btn btn-primary btn-sm">En savoir plus</a>
            </div>
          </div>
        </div>
        <div class="col-md-3 mb-4">
          <div class="card h-100 shadow-sm animate__animated animate__zoomIn animate__delay-2s">
            <img src="https://images.unsplash.com/photo-1504384308090-c894fdcc538d?auto=format&fit=crop&w=400&q=80" class="card-img-top" alt="Cours 3">
            <div class="card-body">
              <h5 class="card-title">Programmation Android avancée</h5>
              <p class="card-text">Perfectionnez vos compétences Android avec des concepts avancés et des projets pratiques.</p>
              <a href="#" class="btn btn-primary btn-sm">En savoir plus</a>
            </div>
          </div>
        </div>
        <!-- Cours de conduite -->
        <div class="col-md-3 mb-4">
          <div class="card h-100 shadow-sm animate__animated animate__zoomIn animate__delay-3s">
            <img src="https://images.unsplash.com/photo-1519681393784-d120267933ba?auto=format&fit=crop&w=400&q=80" class="card-img-top" alt="Cours de conduite">
            <div class="card-body">
              <h5 class="card-title">Cours de conduite</h5>
              <p class="card-text">Apprenez les bases d'une conduite sûre et responsable avec notre cours complet de conduite.</p>
              <a href="#" class="btn btn-primary btn-sm">En savoir plus</a>
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>
<div id="shop" class="container mt-2 mb-5">
    <h2 class="mb-4 text-center">Featured Products</h2>
    <div class="row row-cols-1 row-cols-md-4 g-4">
            {% if(items is not empty) %}
                {% for item in items %}
                    <div class="col">
                        <div class="card h-100 border-0 shadow animate__animated animate__fadeInUp">
                            <div class="position-relative">
                                <img src="{{ asset('upload_images/' ~ item.image ) }}" class="card-img-top rounded-top object-fit-cover" alt="{{ item.designation }}" style="height: 220px; width: 100%; object-fit: cover;">
                                {% if item.price < item.price|default(0) %}
                                    <span class="badge bg-danger position-absolute top-0 end-0 m-2">Sale</span>
                                {% endif %}
                            </div>
                            <div class="card-body d-flex flex-column bg-light">
                                <h5 class="card-title fw-bold text-primary">{{ item.designation|default('No designation') }}</h5>
                                <p class="card-text text-muted small mb-2" style="min-height: 48px;">{{ item.description|default('No description')|length > 60 ? item.description|slice(0, 60) ~ '...' : item.description }}</p>
                                <div class="mb-3">
                                    <span class="h5 text-success fw-bold">{{ (item.price - (item.price * 0.35))|number_format(0, '.', ',') }} FCFA</span>
                                    <span class="text-muted ms-2"><del>{{ item.price|number_format(0, '.', ',') }} FCFA</del>(-35%)</span>
                                </div>
                                <div class="mt-auto">
                                    {% if app.user %}
                                        <button class="btn btn-outline-primary w-100 fw-bold" data-bs-toggle="modal" data-bs-target="#productModal{{ item.id }}">
                                            <i class="fa-solid fa-eye me-1"></i> View Details
                                        </button>
                                    {% else %}
                                        <a class="btn btn-danger w-100 fw-bold" href="{{ path('app_login') }}">
                                            <i class="fa-solid fa-user"></i> Login to view
                                        </a>
                                    {% endif %}
                                </div>
                            </div>
                        </div>
                        {# Modal for product details #}
                        <div class="modal fade" id="productModal{{ item.id }}" tabindex="-1" aria-labelledby="productModalLabel{{ item.id }}" aria-hidden="true">
                            <div class="modal-dialog modal-dialog-centered">
                                <div class="modal-content">
                                    <div class="modal-header bg-primary text-white">
                                        <h5 class="modal-title" id="productModalLabel{{ item.id }}">{{ item.designation }}</h5>
                                        <button type="button" class="btn-close btn-close-white" data-bs-dismiss="modal" aria-label="Close"></button>
                                    </div>
                                    <div class="modal-body">
                                        <img src="{{ asset('upload_images/' ~ item.image ) }}" class="img-fluid rounded mb-3" alt="{{ item.designation }}">
                                        <p class="mb-2"><strong>Description:</strong> {{ item.description|default('No description') }}</p>
                                        <p>
                                            <strong>Price:</strong>
                                            <span class="h5 text-success">{{ (item.price - (item.price * 0.35))|number_format(0, '.', ',') }} FCFA</span>
                                            <span class="text-muted ms-2"><del>{{ item.price|number_format(0, '.', ',') }} FCFA</del>(-35%)</span>
                                        </p>
                                        <hr>
                                        <a class="btn btn-success w-100 fw-bold" href="{{ path('app_order_line_new', {'id': item.id}) }}">
                                            <i class="fa-solid fa-cart-plus me-1"></i> Add to Cart
                                        </a>
                                    </div>
                                    <div class="modal-footer">
                                        <button type="button" class="btn btn-outline-secondary" data-bs-dismiss="modal">Close</button>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                {% endfor %} 
            {% else %}
                <div class="col-12">
                    <div class="alert alert-info text-center">No Course available at the moment.</div>
                </div>
            {% endif %}
    </div>
</div>

<!-- Bootstrap 5.3 JS Bundle -->
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.bundle.min.js"></script>
{% endblock %}
